<Window x:Class="ExcelDBConverter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExcelDBConverter"
        mc:Ignorable="d"
        Title="ExcelConverter" Height="400" Width="750"
        ResizeMode="NoResize">
    <Grid>
        <Frame x:Name="Main" NavigationUIVisibility="Hidden" Margin="0,45,0,0"/>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top" Margin="10,10,0,0" >
            <Button Name="LocalBtn" Content="Local" MinWidth="100" Click="LocalBtn_Click"/>
            <Button Name="DataBaseBtn" Content="DataBase" MinWidth="100" Margin="10,0,0,0" Click="DataBaseBtn_Click"/>
        </StackPanel>
    </Grid>
</Window>
